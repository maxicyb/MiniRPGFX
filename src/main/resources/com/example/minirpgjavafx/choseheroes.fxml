<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="403.0" prefWidth="415.0" styleClass="background_menu" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.ChoseHeroController">
    <stylesheets>
        <URL value="@application.css" />
    </stylesheets>
   <children>
      <Button fx:id="heroButtonW" layoutX="180.0" layoutY="123.0" mnemonicParsing="false" onAction="#pressW" text="Warrior" />
      <Button fx:id="heroButtonH" layoutX="182.0" layoutY="176.0" mnemonicParsing="false" onAction="#pressH" text="Hunter" />
      <Button fx:id="heroButtonM" layoutX="185.0" layoutY="229.0" mnemonicParsing="false" onAction="#pressM" text="Mage" />
      <Label fx:id="labelChoseHeroes" layoutX="86.0" layoutY="59.0" text="Vous devez choisir encore : " />
      <Button fx:id="adventureButton" layoutX="136.0" layoutY="287.0" mnemonicParsing="false" onAction="#startadventure" text="Commencer l'aventure " textFill="#f5580c" visible="false" />
   </children>

</AnchorPane>
